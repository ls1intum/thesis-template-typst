	location = {Baltimore, Maryland, USA},
	numpages = {6},
	pages = {284--289},
	publisher = {Association for Computing Machinery},
	series = {SIGCSE '18},
	title = {ArTEMiS: An Automatic Assessment Management System for Interactive Learning},
